{
  "C_DSL_VIEW_ALL": {
    "prefix": "c_dsl_view_all",
    "body": [
      "<?xml version=\"1.1\" encoding=\"UTF-8\"?><project>",
      "  <actions/>",
      "  <description/>",
      "  <keepDependencies>false</keepDependencies>",
      "  <properties>",
      "    <com.sonyericsson.rebuild.RebuildSettings plugin=\"rebuild@1.33.1\">",
      "      <autoRebuild>false</autoRebuild>",
      "      <rebuildDisabled>false</rebuildDisabled>",
      "    </com.sonyericsson.rebuild.RebuildSettings>",
      "    <hudson.model.ParametersDefinitionProperty>",
      "      <parameterDefinitions>",
      "        <hudson.model.ChoiceParameterDefinition>",
      "          <name>view</name>",
      "          <description>Tab 페이지를 생성합니다.",
      "",
      "all: __All__",
      "dev: 10_Dev, 30_Stg",
      "prd: 10_Prd</description>",
      "          <choices class=\"java.util.Arrays$ArrayList\">",
      "            <a class=\"string-array\">",
      "              <string/>",
      "              <string>all</string>",
      "              <string>dev</string>",
      "              <string>prd</string>",
      "            </a>",
      "          </choices>",
      "        </hudson.model.ChoiceParameterDefinition>",
      "      </parameterDefinitions>",
      "    </hudson.model.ParametersDefinitionProperty>",
      "  </properties>",
      "  <scm class=\"hudson.scm.NullSCM\"/>",
      "  <canRoam>true</canRoam>",
      "  <disabled>false</disabled>",
      "  <blockBuildWhenDownstreamBuilding>false</blockBuildWhenDownstreamBuilding>",
      "  <blockBuildWhenUpstreamBuilding>false</blockBuildWhenUpstreamBuilding>",
      "  <triggers/>",
      "  <concurrentBuild>false</concurrentBuild>",
      "  <builders>",
      "    <javaposse.jobdsl.plugin.ExecuteDslScripts plugin=\"job-dsl@1.79\">",
      "      <scriptText>listView(&apos;ZZ_templates&apos;) {",
      "    filterBuildQueue()",
      "    filterExecutors()",
      "    jobs {",
      "        regex(/_.*/)",
      "    }",
      "    columns {",
      "        status()",
      "        weather()",
      "        name()",
      "        buildButton()",
      "        lastBuildConsole()",
      "        configureProject()",
      "        projectDescriptionColumn {",
      "            displayName(false)",
      "            trim(true)",
      "            columnWidth(120)",
      "         displayLength(120)",
      "            forceWidth(true)",
      "        }",
      "        lastSuccess()",
      "        lastFailure()",
      "        lastDuration()",
      "    }",
      "}",
      "",
      "listView(&apos;_All_&apos;) {",
      "    filterBuildQueue()",
      "    filterExecutors()",
      "    jobs {",
      "        regex(/[a-zA-Z].*/)",
      "    }",
      "    columns {",
      "        status()",
      "        weather()",
      "        name()",
      "        buildButton()",
      "        lastBuildConsole()",
      "        configureProject()",
      "        projectDescriptionColumn {",
      "            displayName(false)",
      "            trim(true)",
      "            columnWidth(120)",
      "         displayLength(120)",
      "            forceWidth(true)",
      "        }",
      "        lastSuccess()",
      "        lastFailure()",
      "        lastDuration()",
      "    }",
      "}",
      "",
      "if (view == &apos;dev&apos;) {",
      "listView(&apos;10_Dev&apos;) {",
      "    filterBuildQueue()",
      "    filterExecutors()",
      "    jobs {",
      "        regex(/[a-zA-Z].*_dev/)",
      "    }",
      "    columns {",
      "        status()",
      "        weather()",
      "        name()",
      "        buildButton()",
      "        lastBuildConsole()",
      "        configureProject()",
      "        projectDescriptionColumn {",
      "            displayName(false)",
      "            trim(true)",
      "            columnWidth(120)",
      "         displayLength(120)",
      "            forceWidth(true)",
      "        }",
      "        lastSuccess()",
      "        lastFailure()",
      "        lastDuration()",
      "    }",
      "}",
      "listView(&apos;30_STG&apos;) {",
      "    filterBuildQueue()",
      "    filterExecutors()",
      "    jobs {",
      "        regex(/[a-zA-Z].*_stg/)",
      "    }",
      "    columns {",
      "        status()",
      "        weather()",
      "        name()",
      "        buildButton()",
      "        lastBuildConsole()",
      "        configureProject()",
      "        projectDescriptionColumn {",
      "            displayName(false)",
      "            trim(true)",
      "            columnWidth(120)",
      "         displayLength(120)",
      "            forceWidth(true)",
      "        }",
      "        lastSuccess()",
      "        lastFailure()",
      "        lastDuration()",
      "    }",
      "}",
      "}",
      "",
      "if (view == &apos;prd&apos;) {",
      "listView(&apos;10_Prd&apos;) {",
      "    filterBuildQueue()",
      "    filterExecutors()",
      "    jobs {",
      "        regex(/[a-zA-Z].*_prd/)",
      "    }",
      "    columns {",
      "        status()",
      "        weather()",
      "        name()",
      "        buildButton()",
      "        lastBuildConsole()",
      "        configureProject()",
      "        projectDescriptionColumn {",
      "            displayName(false)",
      "            trim(true)",
      "            columnWidth(120)",
      "         displayLength(120)",
      "            forceWidth(true)",
      "        }",
      "        lastSuccess()",
      "        lastFailure()",
      "        lastDuration()",
      "    }",
      "}",
      "}</scriptText>",
      "      <usingScriptText>true</usingScriptText>",
      "      <sandbox>false</sandbox>",
      "      <ignoreExisting>false</ignoreExisting>",
      "      <ignoreMissingFiles>false</ignoreMissingFiles>",
      "      <failOnMissingPlugin>false</failOnMissingPlugin>",
      "      <failOnSeedCollision>false</failOnSeedCollision>",
      "      <unstableOnDeprecation>false</unstableOnDeprecation>",
      "      <removedJobAction>IGNORE</removedJobAction>",
      "      <removedViewAction>IGNORE</removedViewAction>",
      "      <removedConfigFilesAction>IGNORE</removedConfigFilesAction>",
      "      <lookupStrategy>SEED_JOB</lookupStrategy>",
      "    </javaposse.jobdsl.plugin.ExecuteDslScripts>",
      "  </builders>",
      "  <publishers/>",
      "  <buildWrappers/>",
      "</project>"
    ],
    "description": "탭 페이지를 생성합니다",
    "when": "plugin.jobdsl.enabled"
  },
  "C_DSL_JOB": {
    "prefix": "c_dsl_job",
    "body": [
      "<?xml version='1.1' encoding='UTF-8'?>",
      "<project>",
      "  <actions/>",
      "  <description>서버 구성을 위한 Job을 생성합니다.&#xd;",
      "&#xd;",
      "    - kaf-AAA_dev</description>",
      "  <keepDependencies>false</keepDependencies>",
      "  <properties>",
      "    <com.sonyericsson.rebuild.RebuildSettings plugin=\"rebuild@320.v5a_0933a_e7d61\">",
      "      <autoRebuild>false</autoRebuild>",
      "      <rebuildDisabled>false</rebuildDisabled>",
      "    </com.sonyericsson.rebuild.RebuildSettings>",
      "    <hudson.model.ParametersDefinitionProperty>",
      "      <parameterDefinitions>",
      "        <hudson.model.StringParameterDefinition>",
      "          <name>PID</name>",
      "          <description>프로젝트 코드를 입력하세요</description>",
      "          <defaultValue>kaf</defaultValue>",
      "          <trim>false</trim>",
      "        </hudson.model.StringParameterDefinition>",
      "        <hudson.model.StringParameterDefinition>",
      "          <name>PROJ_NAME</name>",
      "          <description>프로젝트명을 입력하세요</description>",
      "          <defaultValue>web</defaultValue>",
      "          <trim>false</trim>",
      "        </hudson.model.StringParameterDefinition>",
      "        <hudson.model.StringParameterDefinition>",
      "          <name>ENV</name>",
      "          <description>배포 환경을 입력하세요 [dev, stg, prd]</description>",
      "          <defaultValue>dev</defaultValue>",
      "          <trim>false</trim>",
      "        </hudson.model.StringParameterDefinition>",
      "        <hudson.model.StringParameterDefinition>",
      "          <name>CREDENTIALS</name>",
      "          <description>credentials of the Git </description>",
      "          <defaultValue>kaf-git</defaultValue>",
      "          <trim>false</trim>",
      "        </hudson.model.StringParameterDefinition>",
      "      </parameterDefinitions>",
      "    </hudson.model.ParametersDefinitionProperty>",
      "  </properties>",
      "  <scm class=\"hudson.scm.NullSCM\"/>",
      "  <canRoam>true</canRoam>",
      "  <disabled>false</disabled>",
      "  <blockBuildWhenDownstreamBuilding>false</blockBuildWhenDownstreamBuilding>",
      "  <blockBuildWhenUpstreamBuilding>false</blockBuildWhenUpstreamBuilding>",
      "  <triggers/>",
      "  <concurrentBuild>false</concurrentBuild>",
      "  <builders>",
      "    <javaposse.jobdsl.plugin.ExecuteDslScripts plugin=\"job-dsl@1.83\">",
      "      <scriptText>pipelineJob(&quot;${PID}-${PROJ_NAME}_${ENV}&quot;) {",
      "    description(&quot;${PID}-${PROJ_NAME}_${ENV}&quot;)",
      "    definition {",
      "        cpsScm {",
      "            scm {",
      "                git {",
      "                  remote {",
      "                    name(&apos;origin&apos;)",
      "                    url(&quot;http://$GIT_SERVER/${PID}-${PROJ_NAME}.git&quot;)",
      "                    credentials(&quot;$CREDENTIALS&quot;)",
      "                  }",
      "                  branch(&apos;*/${ENV}&apos;)",
      "                  scriptPath(&quot;Jenkinsfile_dev.groovy&quot;)",
      "                }",
      "            }",
      "            lightweight()",
      "        }",
      "    }",
      "}</scriptText>",
      "      <usingScriptText>true</usingScriptText>",
      "      <sandbox>false</sandbox>",
      "      <ignoreExisting>false</ignoreExisting>",
      "      <ignoreMissingFiles>false</ignoreMissingFiles>",
      "      <failOnMissingPlugin>false</failOnMissingPlugin>",
      "      <failOnSeedCollision>false</failOnSeedCollision>",
      "      <unstableOnDeprecation>false</unstableOnDeprecation>",
      "      <removedJobAction>IGNORE</removedJobAction>",
      "      <removedViewAction>IGNORE</removedViewAction>",
      "      <removedConfigFilesAction>IGNORE</removedConfigFilesAction>",
      "      <lookupStrategy>JENKINS_ROOT</lookupStrategy>",
      "    </javaposse.jobdsl.plugin.ExecuteDslScripts>",
      "  </builders>",
      "  <publishers/>",
      "  <buildWrappers/>",
      "</project>"
    ],
    "description": "파이프라인 Job을 생성합니다",
    "when": "plugin.jobdsl.enabled"
  },
  "C_VIEW_CATEVIEW": {
    "prefix": "c_view_cateview",
    "body": [
